{
  "name": "Phormatter",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Offers a cleaner UI and some functionality for PT",
  "browser_action": {
    "default_icon": "/static/img/icon.ico", // optional
    "default_title": "Phormatter"   // optional; shown in tooltip
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "permissions": [
    "http://phantasytour.com/"
  ]
}